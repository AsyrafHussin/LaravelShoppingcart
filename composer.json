{
    "name": "asyrafhussin/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Asyraf Hussin",
            "email": "asyrafhussin4@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.1.20 - 5.8|^6.0",
        "illuminate/session": "5.1.20 - 5.8|^6.0",
        "illuminate/events": "5.1.20 - 5.8|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0 || ~8.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "AsyrafHussin\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsyrafHussin\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "AsyrafHussin\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "AsyrafHussin\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
